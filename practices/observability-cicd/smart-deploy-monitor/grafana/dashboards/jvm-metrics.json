{
  "title": "Smart Deploy Monitor - JVM Metrics",
  "tags": ["jvm", "java", "memory", "gc"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Heap Memory Usage",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{area=\"heap\"}",
          "legendFormat": "Heap Used",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"}
        },
        {
          "expr": "jvm_memory_committed_bytes{area=\"heap\"}",
          "legendFormat": "Heap Committed",
          "refId": "B",
          "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"}
        },
        {
          "expr": "jvm_memory_max_bytes{area=\"heap\"}",
          "legendFormat": "Heap Max",
          "refId": "C",
          "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {}
        }
      }
    },
    {
      "id": 2,
      "title": "Non-Heap Memory Usage",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{area=\"nonheap\"}",
          "legendFormat": "Non-Heap Used",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"}
        },
        {
          "expr": "jvm_memory_committed_bytes{area=\"nonheap\"}",
          "legendFormat": "Non-Heap Committed",
          "refId": "B",
          "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {}
        }
      }
    },
    {
      "id": 3,
      "title": "GC Collections",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "rate(jvm_gc_pause_seconds_count[5m])",
          "legendFormat": "{{gc}}",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {}
        }
      }
    },
    {
      "id": 4,
      "title": "GC Pause Time",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "rate(jvm_gc_pause_seconds_sum[5m]) / rate(jvm_gc_pause_seconds_count[5m])",
          "legendFormat": "{{gc}} Avg Pause",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {}
        }
      }
    },
    {
      "id": 5,
      "title": "Thread Count",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "jvm_threads_live_threads",
          "legendFormat": "Live Threads",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"}
        },
        {
          "expr": "jvm_threads_daemon_threads",
          "legendFormat": "Daemon Threads",
          "refId": "B",
          "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"}
        },
        {
          "expr": "jvm_threads_peak_threads",
          "legendFormat": "Peak Threads",
          "refId": "C",
          "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {}
        }
      }
    },
    {
      "id": 6,
      "title": "Class Loading",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "targets": [
        {
          "expr": "jvm_classes_loaded_classes",
          "legendFormat": "Loaded Classes",
          "refId": "A",
          "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"}
        },
        {
          "expr": "jvm_classes_unloaded_classes_total",
          "legendFormat": "Unloaded Classes",
          "refId": "B",
          "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {}
        }
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  }
}
